<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:v="clr-namespace:PokemonApi.Mobile.Views"
             x:Class="PokemonApi.Mobile.Pages.PokedexIndex.PokedexIndexPage">
    <v:SelectableListView ItemsSource="{Binding Pokedexes}" SelectItemCommand="{Binding PokedexSelectedCommand}">
        <v:SelectableListView.ItemTemplate>
            <DataTemplate>
                <TextCell Text="{Binding Name}" />
            </DataTemplate>
        </v:SelectableListView.ItemTemplate>
        
    </v:SelectableListView>
</ContentPage>